# LiveKit Cloud Configuration (commented out - using local instead)


# LiveKit Local Configuration
LIVEKIT_URL=ws://localhost:7880
LIVEKIT_API_KEY=devkey
LIVEKIT_API_SECRET=secret
MANAGER_API_URL=http://localhost:8002/toy
# Redis Configuration (using existing manager-api Redis)
REDIS_URL=redis://:redispassword@localhost:6380
REDIS_PASSWORD=redispassword

GOOGLE_API_KEY=your_google_api_key_here
GROQ_API_KEY=your_groq_api_key_here
DEEPGRAM_API_KEY=your_deepgram_api_key_here

# STT Provider Configuration
STT_PROVIDER=groq
# Available options: groq, deepgram
DEEPGRAM_MODEL=nova-2
# Available Deepgram models: nova-2, nova-2-general, nova-2-medical, nova-2-phonecall, nova-2-meeting, enhanced, base

# TTS Provider Configuration
TTS_PROVIDER=groq
# Available options: groq, elevenlabs, edge

# EdgeTTS Configuration (when TTS_PROVIDER=edge)
EDGE_TTS_VOICE=en-US-AvaNeural
# Popular child-friendly voices:
# - en-US-AvaNeural (female, clear)
# - en-US-EmmaNeural (female, warm)
# - en-US-JennyNeural (female, friendly)
# - en-US-AriaNeural (female, conversational)
# - en-GB-LibbyNeural (female, British, child-like)
# - en-GB-MaisieNeural (female, British, young)
# - hi-IN-SwaraNeural (female, Hindi)
# - te-IN-ShrutiNeural (female, Telugu)

EDGE_TTS_RATE=+0%
# Rate adjustment: -50% to +100% (e.g., +20% for faster, -20% for slower)

EDGE_TTS_VOLUME=+0%
# Volume adjustment: -50% to +100% (e.g., +10% for louder, -10% for quieter)

EDGE_TTS_PITCH=+0Hz
# Pitch adjustment: -200Hz to +200Hz (e.g., +50Hz for higher, -50Hz for lower)

EDGE_TTS_SAMPLE_RATE=24000
# Sample rate in Hz: 16000, 24000, 48000

EDGE_TTS_CHANNELS=1
# Number of audio channels: 1 (mono) or 2 (stereo)

# Embedding Model Configuration (for semantic search prewarming)
EMBEDDING_MODEL=all-MiniLM-L6-v2
# Available models: all-MiniLM-L6-v2, all-mpnet-base-v2, multi-qa-MiniLM-L6-cos-v1

# Qdrant Vector Database Configuration
QDRANT_URL=https://your-cluster.qdrant.io:6333
QDRANT_API_KEY=your_qdrant_api_key_here

# Fallback Provider Configuration
FALLBACK_ENABLED=false
# Set to true to enable automatic fallback for STT, LLM, and TTS providers

# LLM Fallback Configuration
FALLBACK_LLM_MODEL=llama-3.1-8b-instant
# Fallback LLM model when primary fails

# TTS Fallback Configuration
TTS_FALLBACK_ENABLED=false
# Set to true to enable TTS fallback (EdgeTTS -> Groq fallback chain)

# Note: STT fallback is automatically configured when FALLBACK_ENABLED=true
# Primary: Groq STT -> Fallback: Deepgram STT