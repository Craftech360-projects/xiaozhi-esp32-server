# Spring Profile Configuration
# Set to 'dev' for development or 'prod' for production
SPRING_PROFILES_ACTIVE=dev

# Server Configuration
SERVER_PORT=8002

# Database Configuration (Override if needed)
# DB_HOST=localhost
# DB_PORT=3306
# DB_NAME=xiaozhi
# DB_USERNAME=root
# DB_PASSWORD=your_password_here

# Redis Configuration (Override if needed)
# REDIS_HOST=localhost
# REDIS_PORT=6379
# REDIS_PASSWORD=your_redis_password

# External Services
XIAOZHI_PYTHON_SERVER_URL=http://localhost:8003

# Vector Database (Qdrant)
QDRANT_URL=your_qdrant_url_here
QDRANT_API_KEY=your_qdrant_api_key_here

# Voyage AI API Key
VOYAGE_API_KEY=your_voyage_api_key_here

# Textbook Upload Directory
TEXTBOOK_UPLOAD_DIR=/path/to/uploadfile/textbooks